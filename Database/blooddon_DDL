UPDATE SYSSTAT.INDEXES
SET NLEAF=1,
    NLEVELS=1,
    FIRSTKEYCARD=1,
    FIRST2KEYCARD=16,
    FIRST3KEYCARD=16,
    FIRST4KEYCARD=-1,
    FULLKEYCARD=16,
    CLUSTERFACTOR=-1.000000,
    CLUSTERRATIO=100,
    SEQUENTIAL_PAGES=0,
    DENSITY=0,
    AVERAGE_SEQUENCE_GAP=0.000000,
    AVERAGE_SEQUENCE_FETCH_GAP=-1.000000,
    AVERAGE_SEQUENCE_PAGES=0.000000,
    AVERAGE_SEQUENCE_FETCH_PAGES=-1.000000,
    AVERAGE_RANDOM_PAGES=1.000000,
    AVERAGE_RANDOM_FETCH_PAGES=-1.000000,
    NUMRIDS=54,
    NUMRIDS_DELETED=38,
    NUM_EMPTY_LEAFS=0,
    AVGPARTITION_CLUSTERRATIO=-1,
    DATAPARTITION_CLUSTERFACTOR=1.000000,
    INDCARD=16
WHERE INDNAME = 'DB2LK_SEQUENCE_IND' AND INDSCHEMA = 'SYSTOOLS'
      AND TABNAME = 'DB2LOOK_INFO' AND TABSCHEMA = 'SYSTOOLS';

UPDATE SYSSTAT.INDEXES
SET NLEAF=1,
    NLEVELS=1,
    FIRSTKEYCARD=1,
    FIRST2KEYCARD=2,
    FIRST3KEYCARD=2,
    FIRST4KEYCARD=3,
    FULLKEYCARD=3,
    CLUSTERFACTOR=-1.000000,
    CLUSTERRATIO=100,
    SEQUENTIAL_PAGES=0,
    DENSITY=0,
    AVERAGE_SEQUENCE_GAP=0.000000,
    AVERAGE_SEQUENCE_FETCH_GAP=-1.000000,
    AVERAGE_SEQUENCE_PAGES=0.000000,
    AVERAGE_SEQUENCE_FETCH_PAGES=-1.000000,
    AVERAGE_RANDOM_PAGES=1.000000,
    AVERAGE_RANDOM_FETCH_PAGES=-1.000000,
    NUMRIDS=54,
    NUMRIDS_DELETED=38,
    NUM_EMPTY_LEAFS=0,
    AVGPARTITION_CLUSTERRATIO=-1,
    DATAPARTITION_CLUSTERFACTOR=1.000000,
    INDCARD=16
WHERE INDNAME = 'DB2LOOK_IND' AND INDSCHEMA = 'SYSTOOLS'
      AND TABNAME = 'DB2LOOK_INFO' AND TABSCHEMA = 'SYSTOOLS';

COMMIT WORK;

-- Mimic functions

UPDATE SYSSTAT.FUNCTIONS
SET ios_per_invoc= -1.0,
    insts_per_invoc= -1.0,
    ios_per_argbyte= -1.0,
    insts_per_argbyte= -1.0,
    percent_argbytes= -1,
    initial_ios= -1.0,
    initial_insts= -1.0,
    cardinality= -1.0;

UPDATE SYSSTAT.FUNCTIONS
SET ios_per_invoc=-1.000000,
    insts_per_invoc=-1.000000,
    ios_per_argbyte=-1.000000,
    insts_per_argbyte=-1.000000,
    percent_argbytes=-1,
    initial_ios=-1.000000,
    initial_insts=-1.000000,
    cardinality=1
WHERE funcname = 'EXPLAIN_GET_MSG' AND funcschema = 'SYSPROC ' AND specificname = 'EXPLAIN_GET_MSG';

UPDATE SYSSTAT.FUNCTIONS
SET ios_per_invoc=-1.000000,
    insts_per_invoc=-1.000000,
    ios_per_argbyte=-1.000000,
    insts_per_argbyte=-1.000000,
    percent_argbytes=-1,
    initial_ios=-1.000000,
    initial_insts=-1.000000,
    cardinality=1
WHERE funcname = 'EXPLAIN_GET_MSG2' AND funcschema = 'SYSPROC ' AND specificname = 'EXPLAIN_GET_MSG2';

COMMIT WORK;

CONNECT RESET;

TERMINATE;


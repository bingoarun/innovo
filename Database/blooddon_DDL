    NUMNULLS=0,
    SUB_COUNT=-1,
    SUB_DELIM_LENGTH=-1,
    AVGCOLLENCHAR=-1,
    AVGCOLLEN=7
WHERE COLNAME = 'DISTRICT' AND TABNAME = 'URGENT' AND TABSCHEMA = 'ARUN    ';


-- Mimic table EVENT 

RUNSTATS ON TABLE "ARUN    "."EVENT" 
	WITH DISTRIBUTION ON COLUMNS (
		"BANK_ID" NUM_FREQVALUES 5 NUM_QUANTILES 5,
		"BDC_EVENT" NUM_FREQVALUES 5 NUM_QUANTILES 5,
		"DELETE" NUM_FREQVALUES 5 NUM_QUANTILES 5,
		"DESCRIPTION" NUM_FREQVALUES 5 NUM_QUANTILES 5,
		"END_DATE" NUM_FREQVALUES 5 NUM_QUANTILES 5,
		"END_TIME" NUM_FREQVALUES 5 NUM_QUANTILES 5,
		"EVENT_ID" NUM_FREQVALUES 5 NUM_QUANTILES 5,
		"EVENT_STATUS" NUM_FREQVALUES 5 NUM_QUANTILES 5,
		"PGM_NAME" NUM_FREQVALUES 5 NUM_QUANTILES 5,
		"STR_DATE" NUM_FREQVALUES 5 NUM_QUANTILES 5,
		"STR_TIME" NUM_FREQVALUES 5 NUM_QUANTILES 5,
		"VENUE" NUM_FREQVALUES 5 NUM_QUANTILES 5);

UPDATE SYSSTAT.INDEXES
SET NLEAF=-1,
    NLEVELS=-1,
    FIRSTKEYCARD=-1,
    FIRST2KEYCARD=-1,
    FIRST3KEYCARD=-1,
    FIRST4KEYCARD=-1,
    FULLKEYCARD=-1,
    CLUSTERFACTOR=-1,
    CLUSTERRATIO=-1,
    SEQUENTIAL_PAGES=-1,
    PAGE_FETCH_PAIRS='',
    DENSITY=-1,
    AVERAGE_SEQUENCE_GAP=-1,
    AVERAGE_SEQUENCE_FETCH_GAP=-1,
    AVERAGE_SEQUENCE_PAGES=-1,
    AVERAGE_SEQUENCE_FETCH_PAGES=-1,
    AVERAGE_RANDOM_PAGES=-1,
    AVERAGE_RANDOM_FETCH_PAGES=-1,
    NUMRIDS=-1,
    NUMRIDS_DELETED=-1,
    NUM_EMPTY_LEAFS=-1,
    AVGPARTITION_CLUSTERRATIO=-1,
    AVGPARTITION_CLUSTERFACTOR=-1.0,
    AVGPARTITION_PAGE_FETCH_PAIRS='',
    DATAPARTITION_CLUSTERFACTOR=1.0
WHERE TABNAME = 'EVENT' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=-1,
    NUMNULLS=-1
WHERE TABNAME = 'EVENT' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.TABLES
SET CARD=5,
    NPAGES=1,
    FPAGES=1,
    OVERFLOW=0,
    ACTIVE_BLOCKS=0
WHERE TABNAME = 'EVENT' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=5,
    NUMNULLS=0,
    SUB_COUNT=-1,
    SUB_DELIM_LENGTH=-1,
    AVGCOLLENCHAR=-1,
    HIGH2KEY='754',
    LOW2KEY='752',
    AVGCOLLEN=8
WHERE COLNAME = 'EVENT_ID' AND TABNAME = 'EVENT' AND TABSCHEMA = 'ARUN    ';


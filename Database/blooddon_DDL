UPDATE SYSSTAT.INDEXES
SET NLEAF=1,
    NLEVELS=1,
    FIRSTKEYCARD=13,
    FIRST2KEYCARD=-1,
    FIRST3KEYCARD=-1,
    FIRST4KEYCARD=-1,
    FULLKEYCARD=13,
    CLUSTERFACTOR=-1.000000,
    CLUSTERRATIO=100,
    SEQUENTIAL_PAGES=0,
    DENSITY=0,
    AVERAGE_SEQUENCE_GAP=0.000000,
    AVERAGE_SEQUENCE_FETCH_GAP=-1.000000,
    AVERAGE_SEQUENCE_PAGES=0.000000,
    AVERAGE_SEQUENCE_FETCH_PAGES=-1.000000,
    AVERAGE_RANDOM_PAGES=1.000000,
    AVERAGE_RANDOM_FETCH_PAGES=-1.000000,
    NUMRIDS=13,
    NUMRIDS_DELETED=0,
    NUM_EMPTY_LEAFS=0,
    AVGPARTITION_CLUSTERRATIO=-1,
    DATAPARTITION_CLUSTERFACTOR=1.000000,
    INDCARD=13
WHERE COLNAMES = '+PINCODE'
      AND TABNAME = 'LOCATION' AND TABSCHEMA = 'ARUN    ';

COMMIT WORK;

-- Mimic table MAIL 

RUNSTATS ON TABLE "ARUN    "."MAIL" 
	WITH DISTRIBUTION ON COLUMNS (
		"DELETE" NUM_FREQVALUES 10 NUM_QUANTILES 11,
		"FROM_ID" NUM_FREQVALUES 10 NUM_QUANTILES 11,
		"M_ID" NUM_FREQVALUES 10 NUM_QUANTILES 11,
		"MESSAGE" NUM_FREQVALUES -1 NUM_QUANTILES -1,
		"READ" NUM_FREQVALUES 10 NUM_QUANTILES 11,
		"SUBJECT" NUM_FREQVALUES 10 NUM_QUANTILES 11,
		"TO_ID" NUM_FREQVALUES 10 NUM_QUANTILES 11);

UPDATE SYSSTAT.INDEXES
SET NLEAF=-1,
    NLEVELS=-1,
    FIRSTKEYCARD=-1,
    FIRST2KEYCARD=-1,
    FIRST3KEYCARD=-1,
    FIRST4KEYCARD=-1,
    FULLKEYCARD=-1,
    CLUSTERFACTOR=-1,
    CLUSTERRATIO=-1,
    SEQUENTIAL_PAGES=-1,
    PAGE_FETCH_PAIRS='',
    DENSITY=-1,
    AVERAGE_SEQUENCE_GAP=-1,
    AVERAGE_SEQUENCE_FETCH_GAP=-1,
    AVERAGE_SEQUENCE_PAGES=-1,
    AVERAGE_SEQUENCE_FETCH_PAGES=-1,
    AVERAGE_RANDOM_PAGES=-1,
    AVERAGE_RANDOM_FETCH_PAGES=-1,
    NUMRIDS=-1,
    NUMRIDS_DELETED=-1,
    NUM_EMPTY_LEAFS=-1,
    AVGPARTITION_CLUSTERRATIO=-1,
    AVGPARTITION_CLUSTERFACTOR=-1.0,
    AVGPARTITION_PAGE_FETCH_PAIRS='',
    DATAPARTITION_CLUSTERFACTOR=1.0
WHERE TABNAME = 'MAIL' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=-1,
    NUMNULLS=-1
WHERE TABNAME = 'MAIL' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.TABLES
SET CARD=11,
    NPAGES=1,
    FPAGES=1,
    OVERFLOW=0,
    ACTIVE_BLOCKS=0
WHERE TABNAME = 'MAIL' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=4,
    NUMNULLS=0,
    SUB_COUNT=-1,
    SUB_DELIM_LENGTH=-1,
    AVGCOLLENCHAR=-1,
    HIGH2KEY='6005',
    LOW2KEY='6001',
    AVGCOLLEN=8
WHERE COLNAME = 'FROM_ID' AND TABNAME = 'MAIL' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=4,
    NUMNULLS=0,
    SUB_COUNT=-1,
    SUB_DELIM_LENGTH=-1,
    AVGCOLLENCHAR=-1,
    HIGH2KEY='6002',
    LOW2KEY='6001',
    AVGCOLLEN=8
WHERE COLNAME = 'TO_ID' AND TABNAME = 'MAIL' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=10,
    NUMNULLS=0,
    SUB_COUNT=-1,
    SUB_DELIM_LENGTH=-1,
    AVGCOLLENCHAR=-1,
    HIGH2KEY='hi mohan',
    LOW2KEY='arun to admin',
    AVGCOLLEN=10
WHERE COLNAME = 'SUBJECT' AND TABNAME = 'MAIL' AND TABSCHEMA = 'ARUN    ';


    AVGCOLLENCHAR=-1,
    AVGCOLLEN=5
WHERE COLNAME = 'REG_DATE' AND TABNAME = 'EVENTCAMP_REGISTRATION' AND TABSCHEMA = 'ARUN    ';

COMMIT WORK;


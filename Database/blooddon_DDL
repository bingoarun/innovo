UPDATE SYSSTAT.INDEXES
SET NLEAF=1,
    NLEVELS=1,
    FIRSTKEYCARD=1,
    FIRST2KEYCARD=7,
    FIRST3KEYCARD=30,
    FIRST4KEYCARD=-1,
    FULLKEYCARD=30,
    CLUSTERFACTOR=-1.000000,
    CLUSTERRATIO=100,
    SEQUENTIAL_PAGES=0,
    DENSITY=0,
    AVERAGE_SEQUENCE_GAP=0.000000,
    AVERAGE_SEQUENCE_FETCH_GAP=-1.000000,
    AVERAGE_SEQUENCE_PAGES=0.000000,
    AVERAGE_SEQUENCE_FETCH_PAGES=-1.000000,
    AVERAGE_RANDOM_PAGES=1.000000,
    AVERAGE_RANDOM_FETCH_PAGES=-1.000000,
    NUMRIDS=82,
    NUMRIDS_DELETED=52,
    NUM_EMPTY_LEAFS=0,
    AVGPARTITION_CLUSTERRATIO=-1,
    DATAPARTITION_CLUSTERFACTOR=1.000000,
    INDCARD=30
WHERE COLNAMES = '+ALTER_ID+EXEC_MODE+EXEC_SEQ'
      AND TABNAME = 'ALTOBJ_INFO' AND TABSCHEMA = 'SYSTOOLS';

COMMIT WORK;

-- Mimic table DB2LOOK_INFO 

RUNSTATS ON TABLE "SYSTOOLS"."DB2LOOK_INFO" 
	WITH DISTRIBUTION ON COLUMNS (
		"CREATION_TIME" NUM_FREQVALUES 10 NUM_QUANTILES 16,
		"OBJ_ATTRIBUTE" NUM_FREQVALUES 10 NUM_QUANTILES 16,
		"OBJ_DEFINER" NUM_FREQVALUES 10 NUM_QUANTILES 16,
		"OBJ_ID" NUM_FREQVALUES 10 NUM_QUANTILES 16,
		"OBJ_NAME" NUM_FREQVALUES 10 NUM_QUANTILES 16,
		"OBJ_QUALIFIER" NUM_FREQVALUES 10 NUM_QUANTILES 16,
		"OBJ_SCHEMA" NUM_FREQVALUES 10 NUM_QUANTILES 16,
		"OBJ_SUBTYPE" NUM_FREQVALUES 10 NUM_QUANTILES 16,
		"OBJ_TYPE" NUM_FREQVALUES 10 NUM_QUANTILES 16,
		"OP_SEQUENCE" NUM_FREQVALUES 10 NUM_QUANTILES 16,
		"OP_TOKEN" NUM_FREQVALUES 10 NUM_QUANTILES 16,
		"RELATED_OBJ_INFO" NUM_FREQVALUES 10 NUM_QUANTILES 16,
		"SQL_OPERATION" NUM_FREQVALUES 10 NUM_QUANTILES 16,
		"SQL_STMT" NUM_FREQVALUES 10 NUM_QUANTILES 16);

UPDATE SYSSTAT.INDEXES
SET NLEAF=-1,
    NLEVELS=-1,
    FIRSTKEYCARD=-1,
    FIRST2KEYCARD=-1,
    FIRST3KEYCARD=-1,
    FIRST4KEYCARD=-1,
    FULLKEYCARD=-1,
    CLUSTERFACTOR=-1,
    CLUSTERRATIO=-1,
    SEQUENTIAL_PAGES=-1,
    PAGE_FETCH_PAIRS='',
    DENSITY=-1,
    AVERAGE_SEQUENCE_GAP=-1,
    AVERAGE_SEQUENCE_FETCH_GAP=-1,
    AVERAGE_SEQUENCE_PAGES=-1,
    AVERAGE_SEQUENCE_FETCH_PAGES=-1,
    AVERAGE_RANDOM_PAGES=-1,
    AVERAGE_RANDOM_FETCH_PAGES=-1,
    NUMRIDS=-1,
    NUMRIDS_DELETED=-1,
    NUM_EMPTY_LEAFS=-1,
    AVGPARTITION_CLUSTERRATIO=-1,
    AVGPARTITION_CLUSTERFACTOR=-1.0,
    AVGPARTITION_PAGE_FETCH_PAIRS='',
    DATAPARTITION_CLUSTERFACTOR=1.0
WHERE TABNAME = 'DB2LOOK_INFO' AND TABSCHEMA = 'SYSTOOLS';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=-1,
    NUMNULLS=-1
WHERE TABNAME = 'DB2LOOK_INFO' AND TABSCHEMA = 'SYSTOOLS';


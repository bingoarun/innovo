------------------------------------------------
 

CREATE TABLE "ARUN    "."BLOODDONOR"  (
		  "DONOR_ID" BIGINT NOT NULL GENERATED ALWAYS AS IDENTITY (  
		    START WITH +400  
		    INCREMENT BY +1  
		    MINVALUE +400  
		    MAXVALUE +9223372036854775807  
		    NO CYCLE  
		    NO CACHE  
		    NO ORDER ) , 
		  "FNAME" VARCHAR(20) NOT NULL , 
		  "LNAME" VARCHAR(20) , 
		  "USERNAME" VARCHAR(30) NOT NULL , 
		  "PASSWORD" VARCHAR(30) NOT NULL , 
		  "BLOOD_GRP" VARCHAR(10) NOT NULL , 
		  "DOB" DATE , 
		  "PREV_BLD_DATE" DATE , 
		  "MAIL_ID" VARCHAR(50) , 
		  "PH_NUM" VARCHAR(20) NOT NULL , 
		  "LOGIN_STATUS" VARCHAR(10) , 
		  "OFFICE_PHNUM" VARCHAR(20) , 
		  "ADDRESS" LONG VARCHAR , 
		  "PINCODE" VARCHAR(10) NOT NULL , 
		  "AVAILABILITY" VARCHAR(10) WITH DEFAULT '1' , 
		  "DELETE" BIGINT )   
		 IN "USERSPACE1" ; 







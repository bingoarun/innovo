-- Mimic table QUERY 

RUNSTATS ON TABLE "ARUN    "."QUERY" 
	WITH DISTRIBUTION ON COLUMNS (
		"FROM_ID" NUM_FREQVALUES 0 NUM_QUANTILES 0,
		"QUERY" NUM_FREQVALUES -1 NUM_QUANTILES -1,
		"QUERY_ID" NUM_FREQVALUES 0 NUM_QUANTILES 0,
		"SUBJECT" NUM_FREQVALUES 0 NUM_QUANTILES 0);

UPDATE SYSSTAT.INDEXES
SET NLEAF=-1,
    NLEVELS=-1,
    FIRSTKEYCARD=-1,
    FIRST2KEYCARD=-1,
    FIRST3KEYCARD=-1,
    FIRST4KEYCARD=-1,
    FULLKEYCARD=-1,
    CLUSTERFACTOR=-1,
    CLUSTERRATIO=-1,
    SEQUENTIAL_PAGES=-1,
    PAGE_FETCH_PAIRS='',
    DENSITY=-1,
    AVERAGE_SEQUENCE_GAP=-1,
    AVERAGE_SEQUENCE_FETCH_GAP=-1,
    AVERAGE_SEQUENCE_PAGES=-1,
    AVERAGE_SEQUENCE_FETCH_PAGES=-1,
    AVERAGE_RANDOM_PAGES=-1,
    AVERAGE_RANDOM_FETCH_PAGES=-1,
    NUMRIDS=-1,
    NUMRIDS_DELETED=-1,
    NUM_EMPTY_LEAFS=-1,
    AVGPARTITION_CLUSTERRATIO=-1,
    AVGPARTITION_CLUSTERFACTOR=-1.0,
    AVGPARTITION_PAGE_FETCH_PAIRS='',
    DATAPARTITION_CLUSTERFACTOR=1.0
WHERE TABNAME = 'QUERY' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=-1,
    NUMNULLS=-1
WHERE TABNAME = 'QUERY' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.TABLES
SET CARD=0,
    NPAGES=0,
    FPAGES=1,
    OVERFLOW=0,
    ACTIVE_BLOCKS=0
WHERE TABNAME = 'QUERY' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=0,
    NUMNULLS=0,
    SUB_COUNT=-1,
    SUB_DELIM_LENGTH=-1,
    AVGCOLLENCHAR=-1,
    AVGCOLLEN=8
WHERE COLNAME = 'QUERY_ID' AND TABNAME = 'QUERY' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=0,
    NUMNULLS=0,
    SUB_COUNT=-1,
    SUB_DELIM_LENGTH=-1,
    AVGCOLLENCHAR=-1,
    AVGCOLLEN=4
WHERE COLNAME = 'FROM_ID' AND TABNAME = 'QUERY' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=0,
    NUMNULLS=0,
    SUB_COUNT=-1,
    SUB_DELIM_LENGTH=-1,
    AVGCOLLENCHAR=-1,
    AVGCOLLEN=4
WHERE COLNAME = 'SUBJECT' AND TABNAME = 'QUERY' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=-1,
    NUMNULLS=0,
    SUB_COUNT=-1,
    SUB_DELIM_LENGTH=-1,
    AVGCOLLENCHAR=-1,
    AVGCOLLEN=72
WHERE COLNAME = 'QUERY' AND TABNAME = 'QUERY' AND TABSCHEMA = 'ARUN    ';


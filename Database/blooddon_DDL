-- Mimic table FEEDBACK 

RUNSTATS ON TABLE "ARUN    "."FEEDBACK" 
	WITH DISTRIBUTION ON COLUMNS (
		"BANK_ID" NUM_FREQVALUES 4 NUM_QUANTILES 4,
		"DELETE" NUM_FREQVALUES 4 NUM_QUANTILES 4,
		"DONATION_DATE" NUM_FREQVALUES 4 NUM_QUANTILES 4,
		"DONOR_ID" NUM_FREQVALUES 4 NUM_QUANTILES 4,
		"FB_ID" NUM_FREQVALUES 4 NUM_QUANTILES 4,
		"FEED_BACK" NUM_FREQVALUES 4 NUM_QUANTILES 4,
		"FLAG" NUM_FREQVALUES 4 NUM_QUANTILES 4);

UPDATE SYSSTAT.INDEXES
SET NLEAF=-1,
    NLEVELS=-1,
    FIRSTKEYCARD=-1,
    FIRST2KEYCARD=-1,
    FIRST3KEYCARD=-1,
    FIRST4KEYCARD=-1,
    FULLKEYCARD=-1,
    CLUSTERFACTOR=-1,
    CLUSTERRATIO=-1,
    SEQUENTIAL_PAGES=-1,
    PAGE_FETCH_PAIRS='',
    DENSITY=-1,
    AVERAGE_SEQUENCE_GAP=-1,
    AVERAGE_SEQUENCE_FETCH_GAP=-1,
    AVERAGE_SEQUENCE_PAGES=-1,
    AVERAGE_SEQUENCE_FETCH_PAGES=-1,
    AVERAGE_RANDOM_PAGES=-1,
    AVERAGE_RANDOM_FETCH_PAGES=-1,
    NUMRIDS=-1,
    NUMRIDS_DELETED=-1,
    NUM_EMPTY_LEAFS=-1,
    AVGPARTITION_CLUSTERRATIO=-1,
    AVGPARTITION_CLUSTERFACTOR=-1.0,
    AVGPARTITION_PAGE_FETCH_PAIRS='',
    DATAPARTITION_CLUSTERFACTOR=1.0
WHERE TABNAME = 'FEEDBACK' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=-1,
    NUMNULLS=-1
WHERE TABNAME = 'FEEDBACK' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.TABLES
SET CARD=4,
    NPAGES=1,
    FPAGES=1,
    OVERFLOW=0,
    ACTIVE_BLOCKS=0
WHERE TABNAME = 'FEEDBACK' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=4,
    NUMNULLS=0,
    SUB_COUNT=-1,
    SUB_DELIM_LENGTH=-1,
    AVGCOLLENCHAR=-1,
    HIGH2KEY='802',
    LOW2KEY='801',
    AVGCOLLEN=8
WHERE COLNAME = 'FB_ID' AND TABNAME = 'FEEDBACK' AND TABSCHEMA = 'ARUN    ';


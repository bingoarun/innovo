-- Mimic table BLOODDONOR 

RUNSTATS ON TABLE "ARUN    "."BLOODDONOR" 
	WITH DISTRIBUTION ON COLUMNS (
		"ADDRESS" NUM_FREQVALUES -1 NUM_QUANTILES -1,
		"AVAILABILITY" NUM_FREQVALUES 9 NUM_QUANTILES 9,
		"BLOOD_GRP" NUM_FREQVALUES 9 NUM_QUANTILES 9,
		"DELETE" NUM_FREQVALUES 9 NUM_QUANTILES 9,

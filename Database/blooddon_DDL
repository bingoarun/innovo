-- Mimic table ADMINISTRATOR 

RUNSTATS ON TABLE "ARUN    "."ADMINISTRATOR" 
	WITH DISTRIBUTION ON COLUMNS (
		"LOGIN_STATUS" NUM_FREQVALUES 1 NUM_QUANTILES 1,
		"PASSWORD" NUM_FREQVALUES 1 NUM_QUANTILES 1,
		"USERNAME" NUM_FREQVALUES 1 NUM_QUANTILES 1);

UPDATE SYSSTAT.INDEXES
SET NLEAF=-1,
    NLEVELS=-1,
    FIRSTKEYCARD=-1,
    FIRST2KEYCARD=-1,
    FIRST3KEYCARD=-1,
    FIRST4KEYCARD=-1,
    FULLKEYCARD=-1,
    CLUSTERFACTOR=-1,
    CLUSTERRATIO=-1,
    SEQUENTIAL_PAGES=-1,
    PAGE_FETCH_PAIRS='',
    DENSITY=-1,
    AVERAGE_SEQUENCE_GAP=-1,
    AVERAGE_SEQUENCE_FETCH_GAP=-1,
    AVERAGE_SEQUENCE_PAGES=-1,
    AVERAGE_SEQUENCE_FETCH_PAGES=-1,
    AVERAGE_RANDOM_PAGES=-1,
    AVERAGE_RANDOM_FETCH_PAGES=-1,
    NUMRIDS=-1,
    NUMRIDS_DELETED=-1,
    NUM_EMPTY_LEAFS=-1,
    AVGPARTITION_CLUSTERRATIO=-1,
    AVGPARTITION_CLUSTERFACTOR=-1.0,
    AVGPARTITION_PAGE_FETCH_PAIRS='',
    DATAPARTITION_CLUSTERFACTOR=1.0
WHERE TABNAME = 'ADMINISTRATOR' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=-1,
    NUMNULLS=-1
WHERE TABNAME = 'ADMINISTRATOR' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.TABLES
SET CARD=1,
    NPAGES=1,
    FPAGES=1,
    OVERFLOW=0,
    ACTIVE_BLOCKS=0
WHERE TABNAME = 'ADMINISTRATOR' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=1,
    NUMNULLS=0,
    SUB_COUNT=-1,
    SUB_DELIM_LENGTH=-1,
    AVGCOLLENCHAR=-1,
    AVGCOLLEN=9
WHERE COLNAME = 'USERNAME' AND TABNAME = 'ADMINISTRATOR' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=1,
    NUMNULLS=0,
    SUB_COUNT=-1,
    SUB_DELIM_LENGTH=-1,
    AVGCOLLENCHAR=-1,
    AVGCOLLEN=9
WHERE COLNAME = 'PASSWORD' AND TABNAME = 'ADMINISTRATOR' AND TABSCHEMA = 'ARUN    ';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=1,
    NUMNULLS=0,
    SUB_COUNT=-1,
    SUB_DELIM_LENGTH=-1,
    AVGCOLLENCHAR=-1,
    AVGCOLLEN=5
WHERE COLNAME = 'LOGIN_STATUS' AND TABNAME = 'ADMINISTRATOR' AND TABSCHEMA = 'ARUN    ';

COMMIT WORK;

UPDATE SYSSTAT.COLDIST
SET COLVALUE = NULL, VALCOUNT= -1
WHERE VALCOUNT <> -1 AND COLNAME = 'USERNAME' AND TABNAME = 'ADMINISTRATOR'
      AND TABSCHEMA = 'ARUN    ';

COMMIT WORK;

UPDATE SYSSTAT.COLDIST
SET COLVALUE = NULL, VALCOUNT= -1
WHERE VALCOUNT <> -1 AND COLNAME = 'PASSWORD' AND TABNAME = 'ADMINISTRATOR'
      AND TABSCHEMA = 'ARUN    ';

COMMIT WORK;

UPDATE SYSSTAT.COLDIST
SET COLVALUE = NULL, VALCOUNT= -1
WHERE VALCOUNT <> -1 AND COLNAME = 'LOGIN_STATUS' AND TABNAME = 'ADMINISTRATOR'
      AND TABSCHEMA = 'ARUN    ';

COMMIT WORK;

COMMIT WORK;


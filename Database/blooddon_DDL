-- Mimic table ALTOBJ_INFO 

RUNSTATS ON TABLE "SYSTOOLS"."ALTOBJ_INFO" 
	WITH DISTRIBUTION ON COLUMNS (
		"ALTER_ID" NUM_FREQVALUES 10 NUM_QUANTILES 20,
		"CREATION_TIME" NUM_FREQVALUES 10 NUM_QUANTILES 20,
		"EXEC_MODE" NUM_FREQVALUES 10 NUM_QUANTILES 20,
		"EXEC_SEQ" NUM_FREQVALUES 10 NUM_QUANTILES 20,
		"OBJ_NAME" NUM_FREQVALUES 10 NUM_QUANTILES 20,
		"OBJ_SCHEMA" NUM_FREQVALUES 10 NUM_QUANTILES 20,
		"OBJ_TYPE" NUM_FREQVALUES 10 NUM_QUANTILES 20,
		"SQL_CODE" NUM_FREQVALUES 10 NUM_QUANTILES 20,
		"SQL_ERRMC" NUM_FREQVALUES 10 NUM_QUANTILES 20,
		"SQL_OPERATION" NUM_FREQVALUES 10 NUM_QUANTILES 20,
		"SQL_STATE" NUM_FREQVALUES 10 NUM_QUANTILES 20,
		"SQL_STMT" NUM_FREQVALUES 10 NUM_QUANTILES 20);

UPDATE SYSSTAT.INDEXES
SET NLEAF=-1,
    NLEVELS=-1,
    FIRSTKEYCARD=-1,
    FIRST2KEYCARD=-1,
    FIRST3KEYCARD=-1,
    FIRST4KEYCARD=-1,
    FULLKEYCARD=-1,
    CLUSTERFACTOR=-1,
    CLUSTERRATIO=-1,
    SEQUENTIAL_PAGES=-1,
    PAGE_FETCH_PAIRS='',
    DENSITY=-1,
    AVERAGE_SEQUENCE_GAP=-1,
    AVERAGE_SEQUENCE_FETCH_GAP=-1,
    AVERAGE_SEQUENCE_PAGES=-1,
    AVERAGE_SEQUENCE_FETCH_PAGES=-1,
    AVERAGE_RANDOM_PAGES=-1,
    AVERAGE_RANDOM_FETCH_PAGES=-1,
    NUMRIDS=-1,
    NUMRIDS_DELETED=-1,
    NUM_EMPTY_LEAFS=-1,
    AVGPARTITION_CLUSTERRATIO=-1,
    AVGPARTITION_CLUSTERFACTOR=-1.0,
    AVGPARTITION_PAGE_FETCH_PAIRS='',
    DATAPARTITION_CLUSTERFACTOR=1.0
WHERE TABNAME = 'ALTOBJ_INFO' AND TABSCHEMA = 'SYSTOOLS';

UPDATE SYSSTAT.COLUMNS
SET COLCARD=-1,
    NUMNULLS=-1
WHERE TABNAME = 'ALTOBJ_INFO' AND TABSCHEMA = 'SYSTOOLS';

